<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="server-url" value="ws://localhost:1337"/>
    <!--engine mode can be 'random' for RandomEngine, 'advance' for AdvanceFirstEngine or 'minimax' for ProbabilisticMinimaxEngine-->
    <add key="engine-mode" value="minimax"/>
    <!--the delay before actions are send to server (so it doesn't insta-play it's turn)-->
    <add key="server-response-delay-ms" value="1000"/>
    <!--a higher should mean better play but it doesn't because the random element of the game (best value found from testing is 2)-->
    <add key="minimax-depth" value="2"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>